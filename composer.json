{
    "name": "creatvstudio/socialite-auth",
    "description": "Provides easy to configure socialite authentication",
    "keywords": [
        "creatvstudio",
        "socialite-auth"
    ],
    "homepage": "https://github.com/creatvstudio/socialite-auth",
    "license": "MIT",
    "type": "library",
    "authors": [
        {
            "name": "Jeffrey Naval",
            "email": "jeff@creatvstudio.ph",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^7.1",
        "illuminate/support": "^8.0"
    },
    "require-dev": {
        "orchestra/testbench": "^6.0",
        "phpunit/phpunit": "^8.0"
    },
    "autoload": {
        "psr-4": {
            "CreatvStudio\\SocialiteAuth\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "CreatvStudio\\SocialiteAuth\\Tests\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit",
        "test-coverage": "vendor/bin/phpunit --coverage-html coverage"

    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "CreatvStudio\\SocialiteAuth\\SocialiteAuthServiceProvider"
            ]
        }
    }
}
